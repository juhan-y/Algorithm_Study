    "t = int(input())\n",
    "dp = [0] * 100\n",
    "dp[0] = 1\n",
    "dp[1] = 1\n",
    "dp[2] = 1\n",
    "\n",
    "for i in range(3, len(dp)):\n",
    "    dp[i] = dp[i - 3] + dp[i - 2]\n",
    "    \n",
    "for _ in range(t):\n",
    "    n = int(input())\n",
    "    print(dp[n - 1])"
